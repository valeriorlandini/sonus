<c74object name='sonus.logistic' category='maps,  math'>

	<digest>Tent map generator </digest>
	<description>Tent map generator </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Valerio Orlandini </metadata>
		<metadata name='tag'>maps</metadata>
		<metadata name='tag'>math</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

		<objarg name='r' optional='1' type='number'>
			<digest>r parameter, from 0 to 4</digest>
			<description>r parameter, from 0 to 4. </description>
		</objarg>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='float'>
			<digest>Depends on inlet</digest>
			<description>Depends on inlet. A number on the first inlet resets the initial value, while on the second inlet sets the mu parameter. </description>
		</method>

		<method name='bang'>
			<digest>Output next value </digest>
			<description>Output next value </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='r' get='1' set='1' type='float64' size='1' >
			<digest>r parameter</digest>
			<description>r parameter. Should be inside [0.0-4.0) range, with divergent trajectories with r > 2.0 and chaotic one with r > 3.56995. </description>
		</attribute>

		<attribute name='x' get='1' set='1' type='float64' size='1' >
			<digest>Initial value</digest>
			<description>Initial value. Should be inside <o>0.0-1.0</o> range. </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
		<seealso name='sonus.tent' />
	</seealsolist>


</c74object>
