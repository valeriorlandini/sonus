<c74object name='sonus.tent' category='maps,  math'>

	<digest>Tent map generator </digest>
	<description>Tent map generator </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Valerio Orlandini </metadata>
		<metadata name='tag'>maps</metadata>
		<metadata name='tag'>math</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

		<objarg name='mu' optional='1' type='number'>
			<digest>mu parameter, from 0 to 2</digest>
			<description>mu parameter, from 0 to 2. </description>
		</objarg>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='float'>
			<digest>Depends on inlet</digest>
			<description>Depends on inlet. A number on the first inlet resets the initial value, while on the second inlet sets the mu parameter. </description>
		</method>

		<method name='bang'>
			<digest>Output next value </digest>
			<description>Output next value </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='x' get='1' set='1' type='float64' size='1' >
			<digest>Initial value</digest>
			<description>Initial value. Should be inside <o>0.0-1.0</o> range. </description>
		</attribute>

		<attribute name='mu' get='1' set='1' type='float64' size='1' >
			<digest>mu parameter</digest>
			<description>mu parameter. Should be inside <o>0.0-2.0</o> range, with chaotic results achieved with values greater than 1.0. </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
		<seealso name='expr' />
		<seealso name='pow' />
	</seealsolist>


</c74object>
