<c74object name='sonus.lorenz~' category='strange attractors,  oscillator'>

	<digest>Lorenz system based oscillator </digest>
	<description>Lorenz system based oscillator </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Valerio Orlandini </metadata>
		<metadata name='tag'>strange attractors</metadata>
		<metadata name='tag'>oscillator</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

		<objarg name='speed' optional='1' type='number'>
			<digest>Evolution speed (0-1)</digest>
			<description>Evolution speed (0-1). </description>
		</objarg>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='float'>
			<digest>Set the evolution speed (0-1)</digest>
			<description>Set the evolution speed (0-1). </description>
		</method>

		<method name='clear'>
			<digest>Reset parameters and initial values to the defaults </digest>
			<description>Reset parameters and initial values to the defaults </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='rho' get='1' set='1' type='float64' size='1' >
			<digest>Value for the rho parameter</digest>
			<description>Value for the rho parameter. The standard value is 28.0, when it is greater than 24.74 the system is chaotic. </description>
		</attribute>

		<attribute name='speed' get='1' set='1' type='float64' size='1' >
			<digest>Evolution speed (0-1) </digest>
			<description>Evolution speed (0-1) </description>
		</attribute>

		<attribute name='sigma' get='1' set='1' type='float64' size='1' >
			<digest>Value for the sigma parameter</digest>
			<description>Value for the sigma parameter. The standard value is 10.0. </description>
		</attribute>

		<attribute name='beta' get='1' set='1' type='float64' size='1' >
			<digest>Value for the rho parameter</digest>
			<description>Value for the rho parameter. The standard value is 2.6667. </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
		<seealso name='sonus.lorenz' />
	</seealsolist>


