<c74object name='sonus.addosc~' category='oscillators'>

	<digest>Additive harmonic oscillator </digest>
	<description>Additive harmonic oscillator </description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Valerio Orlandini </metadata>
		<metadata name='tag'>oscillators</metadata>
	</metadatalist>


	<!--ARGUMENTS-->

	<objarglist>

		<objarg name='frequency' optional='1' type='number'>
			<digest>Oscillator frequency</digest>
			<description>Oscillator frequency. </description>
		</objarg>

		<objarg name='harmonics' optional='1' type='number'>
			<digest>Number of harmonics (1-32)</digest>
			<description>Number of harmonics (1-32). </description>
		</objarg>

	</objarglist>


	<!--MESSAGES-->

	<methodlist>

		<method name='float'>
			<digest>Depends on the inlet </digest>
			<description>Depends on the inlet </description>
		</method>

		<method name='gains'>
			<digest>Sets all the gains</digest>
			<description>Sets all the gains. </description>
		</method>

		<method name='phases'>
			<digest>Sets all the phases</digest>
			<description>Sets all the phases. </description>
		</method>

		<method name='gain'>
			<digest>Set the gain of the specified harmonic</digest>
			<description>Set the gain of the specified harmonic. </description>
		</method>

		<method name='phase'>
			<digest>Set the phase of the specified harmonic</digest>
			<description>Set the phase of the specified harmonic. </description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='normalize' get='1' set='1' type='long' size='1' >
			<digest>If enabled, gains are normalized so that output does not clip</digest>
			<description>If enabled, gains are normalized so that output does not clip. </description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
		<seealso name='sonus.blosc~' />
		<seealso name='sonus.nwosc~' />
		<seealso name='sonus.rsosc~' />
		<seealso name='cycle~' />
	</seealsolist>


</c74object>
